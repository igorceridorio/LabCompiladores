class A {
   
   public void m1() {
      write( 1 );
   }   
   public void m2(int n) {
      write( n );
   }   

}


class B extends A {
   
   public void m2(int n) {
      write( n );
      super.m2(n + 1);
   }   

}


class C extends B {
   
   public void m1() {
      super.m1();
      write( 2 );
   }   
   public void m3() {
      this.m1();
      write( 1 );
      write( 2 );
   }   

}


class Program {
   
   public void run() {
      A a;
      B b;
      C c;
      
      writeln("");
      writeln("Ok-ger12");
      writeln("The output should be :");
      writeln("1 2 1 2 1 2 1 2");
      b = new B();
      b.m2(1);
      c = new C();
      c.m1();
      c.m3();
   }   

}


