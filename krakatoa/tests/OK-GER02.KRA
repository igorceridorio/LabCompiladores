  @filename   ok-ger02.kra
      
  @comment    Testa se o codigo para os operadores aritmeticos sao
  gerados corretamente.
  */

class A {
   
   public void m() {
      write( 6 );
      write( 1 );
      write( 1 + 1 );
      write( 4 - 1 );
      write( (6 - 3) + 1 );
      write( 10 / 2 );
      write( 2 * 3 );
   }   

}


class Program {
   
   public void run() {
      A a;
      
      writeln("");
      writeln("Ok-ger02");
      writeln("The output should be :");
      writeln("6 1 2 3 4 5 6" );
      a = new A();
      a.m();
   }   

}


