class A {
   static private int n;
   static public int get() {
       return A.n;
   }
   static public void set( int n ) {
       A.n = n;
   }
   
}

class B extends A {
   static private int n;
   static public int get(int n) {
       return B.n + n;
   }
   static public boolean set( boolean ok, int n ) {
       B.n = n;
       return ! ok;
   }
   
}


